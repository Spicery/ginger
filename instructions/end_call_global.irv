long var = ToLong( pc[ 1 ] );
Ref r = ((IdentClass *)( pc[ 2 ] )) -> valof;
VMCOUNT = VMVP - ( Ref * )( VMSP[ var ] );
{    
    Ref *ptr;
    if ( !IsObj( r ) ) call_error( r );
    ptr = RefToPtr4( r );
    if ( !IsFunction( *ptr ) ) call_error( r );
    VMLINK = pc + 3;
    VMLINKFUNC = VMPCFUNC;
	VMPCFUNC = ptr;
    RETURN( ptr + 1 );
}
