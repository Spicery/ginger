
CC=@CXX@
CXX=@CXX@


CXXFLAGS=-g -I../../CppUnitLite2/src  -I../hpp -L../cpp -Wall -Wnon-virtual-dtor # -Weffc++

CFLAGS=-Wmissing-prototypes 

OBJS=maintest.o transdoubletest.o saxtestlite.o

all: maintest

check: maintest
	./maintest

# $@ is make shorthand for the target.
# $@ is make shorthand for the dependencies
maintest: $(OBJS)
	$(CC) $(CXXFLAGS) -o $@ $^ ../../CppUnitLite2/cppunitlite.a -lgng
	
clean:
	rm -f *.o
	rm -f maintest
