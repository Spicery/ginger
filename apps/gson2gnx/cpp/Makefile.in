prefix=@prefix@
exec_prefix=@prefix@
datarootdir=@datarootdir@
PACKAGE_TARNAME=@PACKAGE_TARNAME@
INSTALL_BIN=@bindir@
INSTALL_TOOL=@libexecdir@/@PACKAGE_TARNAME@
INSTALL_LIB=@datarootdir@/@PACKAGE_TARNAME@
INSTALL_DOC=@docdir@


CC=@CXX@
CXX=@CXX@


INSTALLDIRS=-DINSTALL_BIN='"$(INSTALL_BIN)"' -DINSTALL_LIB='"$(INSTALL_LIB)"' -DINSTALL_TOOL='"$(INSTALL_TOOL)"'

CXXFLAGS=$(INSTALLDIRS) -I../hpp -I../../libgng/hpp -Wall -g
CFLAGS=-Wmissing-prototypes 

OBJS=gson2gnx.o item_factory.o item.o toktype.o

gson2gnx: $(OBJS)
	$(CC) $(CXXFLAGS) $(BOOST_CPPFLAGS) -o gson2gnx $(OBJS) ../../libgng/cpp/libgng.a
	
clean:
	rm -f *.o
	rm -f gson2gnx
