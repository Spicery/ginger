prefix=@prefix@
exec_prefix=@prefix@
datarootdir=@datarootdir@
PACKAGE_TARNAME=@PACKAGE_TARNAME@
INSTALL_BIN=@bindir@
INSTALL_LIB=@datarootdir@/@PACKAGE_TARNAME@
INSTALL_DOC=@docdir@

EXEC_DIR="$(INSTALL_BIN)"


CC=@CXX@

# -Weffc++
CXXFLAGS=-DINSTALL_BIN='"$(INSTALL_BIN)"' -DINSTALL_LIB='"$(INSTALL_LIB)"' -DEXEC_DIR='$(EXEC_DIR)' -g -I../hpp -I../../libgng/hpp -I/usr/local/boost_1_43_0 -Wall -Wnon-virtual-dtor
CFLAGS=-Wmissing-prototypes 

OBJS=fetchgnx.o search.o folderscan.o projectcache.o packagecache.o varinfo.o importinfo.o importsetinfo.o

all: fetchgnx

# $@ is make shorthand for the target.
# $@ is make shorthand for the dependencies
fetchgnx: $(OBJS)
	@CXX@ $(CXXFLAGS) $(BOOST_CPPFLAGS) -o $@ $(OBJS) ../../libgng/cpp/libgng.a

clean:
	rm -f *.o
	rm -f fetchgnx

#	Nothing needs doing.
check:
