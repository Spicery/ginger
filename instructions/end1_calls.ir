Ref r = *( VMVP-- );
long var = ToLong( pc[ 1 ] );
VMCOUNT = ( VMVP - ToRefRef( LOCAL( var ) ) ) - 1;
{    
    Ref *ptr;
    if ( !IsObj( r ) ) call_error( r );
    ptr = RefToPtr4( r );
    if ( !IsFunction( *ptr ) ) call_error( r );
    VMLINK = pc + 2;
    VMLINKFUNC = VMPCFUNC;
    VMPCFUNC = ptr;
    RETURN( ptr + 1 );
}
