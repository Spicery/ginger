prefix=@prefix@
exec_prefix=@prefix@
datarootdir=@datarootdir@
PACKAGE_TARNAME=@PACKAGE_TARNAME@
INSTALL_BIN=@bindir@
INSTALL_LIB=@datarootdir@/@PACKAGE_TARNAME@
INSTALL_DOC=@docdir@
CC=@CXX@


CXXFLAGS=-DINSTALL_BIN='"$(INSTALL_BIN)"' -DINSTALL_LIB='"$(INSTALL_LIB)"' -I../hpp -I../../libgng/hpp -I/usr/local/boost_1_43_0 -Wall -g
CFLAGS=-Wmissing-prototypes 

OBJS=lnx2csv.o

lnx2csv: $(OBJS)
	g++ $(CXXFLAGS) $(BOOST_CPPFLAGS) -o lnx2csv $(OBJS) ../../libgng/cpp/libgng.a

clean:
	rm -f *.o
	rm -f lnx2csv
