long n = ToLong( *( ++pc ) );
#ifdef DBG_SPECIAL
fprintf(
	stderr,
	"Difference between %lu and %lu (held in %ld)\n",
	ToULong( VMVP ),
	ToULong( VMSP[n] ),
	n
);
#endif
if ( VMVP == VMSP[n] ) RETURN( pc + 1 );
throw Mishap( "Wrong number of results (needed 0)" ).culprit( "#Results", "" + ( VMVP - ToRefRef( VMSP[n] ) ) );
RETURN( NULL );	// stupid compiler
